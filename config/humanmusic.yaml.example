bot:
  client_id: your_client_id_here
  token: your_bot_token_here
  ops:
    - your_discord_id_here
  error_spam_threshold: 5
  modules:
    - admin
    - voicecontrol
    - music

database:
  db_type: sqlite
  db_name: /data/humanmusic.db

audio:
  buffer_limit: 5

search:
  ytkey: your_youtube_key

notifications:
  # Discord user IDs to DM when unhandled errors occur
  error_recipients:
    - your_discord_id_here
